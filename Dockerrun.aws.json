{
	"AWSEBDockerrunVersion": 2,
	"containerDefinitions": [
		{
			"name": "fetch",
			"image": "madou/gw2armory-fetch",
			"essential": true,
			"memory": 128,
			"environment": [
				{
					"name": "ENV",
					"value": "BETA"
				}
			]
		},
		{
			"name": "server",
			"image": "madou/gw2armory-server",
			"essential": true,
			"memory": 128,
			"links": [
        "fetch"
      ],
			"environment": [
				{
					"name": "ENV",
					"value": "BETA"
				}
			],
      "portMappings": [
	      {
	      	"hostPort": 80,
	      	"containerPort": 80
	    	}
    	]
		}
	]
}