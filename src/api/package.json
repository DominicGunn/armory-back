    "memoizee": "^0.4.1",
    "moment": "^2.14.1",
    "mysql": "^2.8.0",
    "nodemailer": "^2.6.0",
    "nodemailer-ses-transport": "^1.4.0",
    "password-hash-and-salt": "^0.1.2",
    "path": "^0.11.14",
    "q": "^1.4.1",
    "restify": "^3.0.3",
    "restify-oauth2": "^4.0.3",
    "sequelize": "^3.6.0",
    "sequelize-cli": "^2.4.0",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "mocha": "^3.1.1",
    "nodemon": "^1.4.1",
    "proxyquire": "^1.7.10",
    "run-sequence": "^1.1.2",
