{
  "name": "armory-api",
  "version": "1.0.0",
  "description": "Api server for api.gw2armory.com",
  "author": "madou",
  "scripts": {
    "start": "node ./dist",
    "build": "babel src --out-dir dist --ignore spec.js,int.js",
    "dev": "ENV=DEV babel-node ./src -s",
    "test": "mocha \"src/**/*.spec.js\" && npm run test:int",
    "test:int": "mocha \"src/**/*.int.js\"",
    "tdd": "mocha \"src/**/*.spec.js\" --watch",
    "tdd:int": "npm run test:int -- --watch"
  },
  "dependencies": {
    "aws-sdk": "^2.5.3",
    "axios": "^0.5.4",
    "babel-polyfill": "^6.20.0",
    "fs": "0.0.2",
    "gotta-validate": "^0.2.x",
    "lodash": "^4.16.4",
    "memoizee": "^0.4.1",
    "moment": "^2.14.1",
    "mysql": "^2.8.0",
    "nodemailer": "^2.6.0",
    "nodemailer-ses-transport": "^1.4.0",
    "password-hash-and-salt": "^0.1.2",
    "path": "^0.11.14",
    "q": "^1.4.1",
    "restify": "^3.0.3",
    "restify-oauth2": "^4.0.3",
    "sequelize": "^3.6.0",
    "sequelize-cli": "^2.4.0",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "app-module-path": "^2.1.0",
    "babel-cli": "^6.18.0",
    "babel-plugin-syntax-flow": "^6.18.0",
    "babel-plugin-transform-flow-strip-types": "^6.18.0",
    "babel-plugin-transform-object-rest-spread": "^6.20.2",
    "babel-preset-latest": "^6.16.0",
    "babel-register": "^6.18.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "mocha": "^3.1.1",
    "nodemon": "^1.4.1",
    "proxyquire": "^1.7.10",
    "run-sequence": "^1.1.2",
    "sinon": "^1.17.6",
    "sinon-chai": "^2.8.0",
    "sqlite3": "^3.0.10"
  }
}
