{
  "name": "armory-fetch",
  "version": "1.0.0",
  "description": "Fetch micro-service for api.gw2armory.com",
  "author": "madou",
  "scripts": {
    "start": "node ./dist",
    "build": "babel src --out-dir dist --ignore spec.js,int.js",
    "dev": "babel-node ./src",
    "test": "mocha \"src/**/*.spec.js\" --compilers js:babel-register && npm run test:int",
    "test:int": "mocha \"src/**/*.int.js\" --compilers js:babel-register",
    "tdd": "mocha \"src/**/*.spec.js\" --watch --compilers js:babel-register",
    "tdd:int": "npm run test:int -- --watch --compilers js:babel-register"
  },
  "dependencies": {
    "axios": "^0.5.4",
    "babel-polyfill": "^6.20.0",
    "lodash": "^4.16.4",
    "mysql": "^2.8.0",
    "path": "^0.11.14",
    "q": "^1.4.1",
    "restify": "^3.0.3",
    "sequelize": "^3.6.0",
    "sequelize-cli": "^2.4.0",
    "throat": "^2.0.2",
    "yargs": "^3.29.0"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-plugin-syntax-flow": "^6.18.0",
    "babel-plugin-transform-flow-strip-types": "^6.18.0",
    "babel-plugin-transform-object-rest-spread": "^6.20.2",
    "babel-preset-latest": "^6.16.0",
    "babel-register": "^6.18.0",
    "chai": "^3.5.0",
    "mocha": "^3.1.0",
    "mockery": "^1.4.0",
    "proxyquire": "^1.7.10",
    "sinon": "^1.17.6",
    "sinon-chai": "^2.8.0",
    "sqlite3": "^3.0.10"
  }
}
